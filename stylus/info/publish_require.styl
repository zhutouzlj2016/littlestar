body
	background #EFEFF4
	color #323232
	font-family Microsoft Yahei
	
.header
	width 100%
	height 40px
	padding-top 20px
	background-color #ffffff
	position relative
	border-bottom 1px solid #E9EAED
	.icon-back
		position absolute
		top 20px
		left 0px
		width 40px
		height 40px
		background url(../../img/login/nav_back_icon@2x.png) no-repeat center
		background-size 10px 18px	
	.header_title
		display block
		width 100%
		height 40px
		line-height 40px
		text-align center
		font-size 20px
.demand_cells
	margin-top 10px
	padding 0px 15px 
	.demand_cell
		padding-bottom 10px
		border-radius 5px
		box-shadow 0 0 1px 1px #e0e0e0
		background-color #fff
		.demand-title
			display flex
			height 45px
			line-height 45px
			border-bottom 1px solid #EBEBEE
			padding 0px 15px
			.d-t-name
				flex 1
				font-size 14px
			.d-t-tag
				color #C6C6C6
				font-size 10px
		.demand-info
			font-size 12px
			margin 0px
			line-height 14px
			padding 10px 15px
			
	.demand-footer
		padding 0px 15px
		display flex
		height 25px
		line-height 25px
		.text-time
			flex 1
		.btn-detail
			flex 1
			.btn-detail-word
				margin 0 auto
				width 90px
				height 25px
				background #EFEFF4
				text-align center
				font-size 12px
				border-radius 3px
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			