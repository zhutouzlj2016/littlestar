body
	background #EFEFF4
	color #323232
	font-family Microsoft Yahei
	
.header
	width 100%
	height 40px
	padding-top 20px
	background-color #ffffff
	position relative
	border-bottom 1px solid #E9EAED
	.icon-back
		position absolute
		top 20px
		left 0px
		width 40px
		height 40px
		background url(../../img/login/nav_back_icon@2x.png) no-repeat center
		background-size 10px 18px	
	.header_title
		display block
		width 100%
		height 40px
		line-height 40px
		text-align center
		font-size 20px
.cells
	background-color #fff
	padding 15px
	margin-top 10px
	border-top 1px solid #E9EAED
	border-bottom 1px solid #E9EAED
	.cell
		padding-left 130px
		height 85px
		background url(../../img/home/content_img_list@2x.png) no-repeat left center
		background-size 120px 85px
		font-size 12px
		.cell-header
			display flex
			.cell-header-left
				flex 7
				height 30px
				line-height 30px
				font-size 16px
			.cell-header-right
				flex 3
				font-size 10px
				color #828282
				text-align right
				line-height 30px
		.cell-content
			margin 0px
			color #828282
		.cell-botton
			height 16px
			line-height 16px
			width 55px
			border 1px solid #EB7946
			color #EB7946
			border-radius 3px
			margin 5px 0px
			text-align center
		.cell-foot
			color #828282
			display flex
			.foot-left
				flex 4
				.foot-style
					color #EB7946
			.foot-right
				flex 3	
				text-align right