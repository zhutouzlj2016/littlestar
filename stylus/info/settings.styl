body
	background #EFEFF4
	color #323232
	
.header
	width 100%
	height 40px
	padding-top 20px
	background-color #ffffff
	position relative
	border-bottom 1px solid #E9EAED
	.icon-back
		position absolute
		top 20px
		left 0px
		width 40px
		height 40px
		background url(../../img/login/nav_back_icon@2x.png) no-repeat center
		background-size 10px 18px	
	.header_title
		display block
		width 100%
		height 40px
		line-height 40px
		text-align center
		font-size 20px
.info_cell
	height 50px
	line-height 50px
	font-size 14px
	padding 0px 15px
	margin-top 10px
	border-top 1px solid #E9EAED
	border-bottom 1px solid #E9EAED
	background #fff
	position relative
	display flex
	.info_cell_left
		flex 9
	.info_cell_right
		flex 2
		color #C7C7C7
		&:after
			content ""
			width 20px
			height 36px
			position absolute
			top 50%
			right 15px
			margin-top -18px
			background url(../../img/info/content_list_right_icon@2x.png) no-repeat center
			background-size 8px 15px
.info-cell-foot
	height 50px
	line-height 50px
	font-size 14px
	margin-top 20px
	border-top 1px solid #E9EAED
	border-bottom 1px solid #E9EAED
	background #fff
	text-align center
		
.mast
	position absolute
	display none
	top 0
	right 0
	left 0
	bottom 0
	background rgba(60,60,60,0.5)
	.success_box
		width 270px
		height 110px
		background #fff
		border-radius 8px
		overflow hidden
		border 1px solid #EBEBEE
		position absolute
		top 50%
		left 50%
		margin-top -56px
		margin-left -136px	
		.success_top
			font-size 12px
			text-align center
			height 64px
			line-height 64px
			border-bottom 1px solid #EBEBEE
		.yesno
			height 45px
			display flex
			
			line-height 45px
			font-size 16px
			.no
				flex 1
				color #808080
				text-align center
				position relative
				&:after
					content ""
					width 1px
					height 45px
					background #EBEBEE
					position absolute
					top 0
					right 0
			.yes
				flex 1
				color #ED7139
				text-align center
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		