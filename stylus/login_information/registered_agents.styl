body
	background #EFEFF4
	color #323232
	
.header
	width 100%
	height 40px
	padding-top 20px
	background-color #ffffff
	position relative
	border-bottom 1px solid #E9EAED
	.icon-back
		position absolute
		top 20px
		left 0px
		width 40px
		height 40px
		background url(../../img/login/nav_back_icon@2x.png) no-repeat center
		background-size 10px 18px
	.header_title
		display block
		width 100%
		height 40px
		line-height 40px
		text-align center
		color #EE7740
		font-size 20px	
.project
	height 30px 
	line-height 30px 
	font-size 12px
	color #C4C4C4
	padding 0px 15px
.project_box
	height 240px 
	background #fff
	border-bottom 1px solid #E9EAED
	border-top 1px solid #E9EAED
	.project_avatar
		width 120px
		height 65px 
		padding 10px 0px
		margin 0 auto
		border-bottom 1px solid #F0F0F0
		margin-bottom 10px
		.add_avatar
			background #868686
			width 65px
			height 65px
			border-radius 50%
			line-height 65px
			font-size 12px
			color #DADADA
			text-align center
			margin 0 auto
			overflow hidden
.project_input
	display block
	padding 0px 10px
	width 60%
	heught 30px 
	margin 0 auto
	border none
	outline none
	background-color #EFEFF4
	color #C3C3C4
	line-height 30px
	font-size 12px
	border-radius 2px
	margin-bottom 5px
.project_wx
	padding 10px 15px
	border-bottom 1px solid #E9EAED
	border-top 1px solid #E9EAED
	background-color #fff
	.contents
		display flex
		margin-top 20px
		height 64px
		.content
			flex 1
			height 64px
			.content_box
				background-color #878787
				height 64px
				width 95px
				line-height 64px
				text-align center
				border-radius 6px
				color #DADADA
				font-size 12px
			.content_box1
				float left
			.content_box2
				margin 0 auto
			.content_box3
				float right
.login_box
	margin-top 45px	
	padding 0px 15px 
	.login
		background #EA5404
		border-radius 8px
		color #fff
		height 45px
		text-align center
		line-height 45px 
		font-size 18px		
		
.mast
	position absolute
	display none
	top 0
	right 0
	left 0
	bottom 0
	background #B2B2B2
	.success_box
		width 270px
		height 110px
		background #fff
		border-radius 8px
		overflow hidden
		border 1px solid #EBEBEE
		position absolute
		top 50%
		left 50%
		margin-top -56px
		margin-left -136px	
		.success_top
			font-size 12px
			text-align center
			height 64px
			line-height 64px
			border-bottom 1px solid #EBEBEE
		.yesno
			height 45px
			color #ED7139
			line-height 45px
			font-size 16px
			color #ED7139
			text-align center		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
			
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	