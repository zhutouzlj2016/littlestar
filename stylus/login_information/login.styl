body
	background #EFEFF4
	color #323232
	font-family Microsoft Yahei
.header
	width 100%
	height 40px
	padding-top 20px
	background-color #ffffff
	position relative
	border-bottom 1px solid #E9EAED
	.icon-back
		position absolute
		top 20px
		left 0px
		width 40px
		height 40px
		background url(../../img/login/nav_back_icon@2x.png) no-repeat center
		background-size 10px 18px
	.header_title
		display block
		width 100%
		height 40px
		line-height 40px
		text-align center
		color #EE7740
		font-size 20px
.form-list
	margin-top 20px
	background #fff
	padding 0px 15px
	border-bottom 1px solid #E9EAED
	border-top 1px solid #E9EAED
	.form-cell
		padding-left 30px 
		height 48px
		display flex
		line-height 48px
		.form-cell-input
			height 20px
			border none
			font-size 14px
			margin-top 13px
			flex 1
			outline none
	.form-cell1
		background url(../../img/login/content_ID_icon@2x.png) no-repeat left center
		background-size 20px 20px 
		position relative
		&:after
			content ""
			width 100%
			height 1px
			background #F7F7F7
			position absolute
			bottom 0px
			left 0px
	.form-cell2
		background url(../../img/login/content_password_icon@2x.png) no-repeat left 2px center
		background-size 16px 20px 		
.login_box
	margin-top 45px	
	padding 0px 15px 
	.login
		background #C7C7C7
		border-radius 8px
		color #fff
		height 45px
		text-align center
		line-height 45px 
		font-size 18px			
.regsiter
	padding 0px 20px
	display flex
	margin-top 15px
	font-size 14px
	.left
		color #FC9D3C
		flex 1
		height 30px
		line-height 30px
	.right
		line-height 30px
		height 30px
		flex 2
		color #808081
		text-align right
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
		
			
		