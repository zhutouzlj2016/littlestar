body
	background #EFEFF4
	color #323232
.tab_box
	height 40px
	padding-top 20px
	background-color #fff
	.tab_bar
		display flex
		width 70%
		height 28px
		margin 0 auto
		padding 0px
		list-style none
		border-radius 5px
		border 1px solid #ED692B
		color #ED692B
		.tab_bar-cell
			position relative
			flex 1
			line-height 28px
			text-align center
			&:after
				content ''
				position absolute
				top 0
				right 0
				display block
				width 1px
				height 28px
				background-color #ED692B
			&:last-child:after
				content ''
				display none
			&.active
				background-color #ED692B
				color #fff

.demand_cells
	margin-top 10px
	padding 0px 15px 
	.demand_cell
		padding-bottom 10px
		border-radius 5px
		box-shadow 0 0 1px 1px #e0e0e0
		background-color #fff
		.demand-title
			display flex
			height 45px
			line-height 45px
			padding 0px 15px
			border-bottom 1px solid #EBEBEE
			.d-t-name
				flex 1
				font-size 14px
			.d-t-tag
				color #fff
				font-size 10px
				padding 2px 0px
				width 50px
				margin 13px 0
				height 15px
				line-height 19px
				text-align center
				background-color #15B755
				display inline-block
				border-radius 5px
			    
			      
			      
		.demand-info
			font-size 12px
			margin 0px
			line-height 14px
			padding 10px 15px
			
	.demand-footer
		display flex
		height 25px
		line-height 25px
		padding 0px 15px
		.text-time
			flex 1
			color #828282
			font-size 12px
			text-align right 
		.btn-detail
			flex 1
			.btn-detail-word
				margin 0 auto
				width 90px
				height 25px
				background #EFEFF4
				text-align center
				font-size 12px
				border-radius 3px
